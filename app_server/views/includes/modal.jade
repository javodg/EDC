//modal
div.modal.fade#myModal(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
	div.modal-dialog(role='document')
		div.modal-content
			div.modal-header
				span(aria-hidden='true') &times;
				h4.modal-title#myModalLabel En contruccion
			div.modal-body En contruccion
			div.modal-footer
				button.btn.btn-default(type='button', data-dismiss='modal') Cerrar